<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <artifactId>zhengcheng-parent</artifactId>
        <groupId>com.zhengcheng</groupId>
        <version>4.2.0</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>
    <packaging>jar</packaging>
    <artifactId>zc-web-spring-boot-starter</artifactId>
    <description>WEB模块通用组件</description>

    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-aop</artifactId>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-actuator</artifactId>
        </dependency>
        <dependency>
            <groupId>com.zhengcheng</groupId>
            <artifactId>zc-common-spring-boot-starter</artifactId>
        </dependency>
        <!--Mysql数据库通用组件-->
        <dependency>
            <groupId>com.zhengcheng</groupId>
            <artifactId>zc-db-spring-boot-starter</artifactId>
        </dependency>
        <!--缓存通用组件-->
        <dependency>
            <groupId>com.zhengcheng</groupId>
            <artifactId>zc-redis-spring-boot-starter</artifactId>
        </dependency>
        <!--多线程通用组件-->
        <dependency>
            <groupId>com.zhengcheng</groupId>
            <artifactId>zc-async-spring-boot-starter</artifactId>
        </dependency>
        <!--远程通信通用组件-->
        <dependency>
            <groupId>com.zhengcheng</groupId>
            <artifactId>zc-feign-spring-boot-starter</artifactId>
        </dependency>
        <!--认证客户端通用组件-->
        <dependency>
            <groupId>com.zhengcheng</groupId>
            <artifactId>zc-auth-client-spring-boot-starter</artifactId>
        </dependency>
        <!--swagger通用组件-->
        <dependency>
            <groupId>com.zhengcheng</groupId>
            <artifactId>zc-swagger-spring-boot-starter</artifactId>
        </dependency>
    </dependencies>

    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-source-plugin</artifactId>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>

</project>
